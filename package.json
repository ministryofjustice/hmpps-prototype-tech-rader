{
  "name": "hmpps-template-prototyping",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon server.js --config ./package.json",
    "test": "jest",
    "test:watch": "jest --notify --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "govuk-frontend": "^3.11.0",
    "http-errors": "~1.6.3",
    "jest": "^26.6.3",
    "morgan": "~1.9.1",
    "node-sass-middleware": "^0.11.0",
    "nunjucks": "^3.2.3",
    "sass": "^1.32.8"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "watch": [
      "views",
      "routes",
      "public"
    ]
  },
  "ext": "js,json,html,css,scss,sass",
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
